{
  "appId": "com.mdlv.mdlv",
  "appName": "Mieles de la Villa",
  "bundledWebRuntime": false,
  "npmClient": "npm",
  "webDir": "www",
  "android": {
    "allowMixedContent": true
  },
  "sever" : {
    "iosScheme": "ionic",
    "androidScheme": "http"
  },
  "plugins": {
    "SplashScreen": {
      "backgroundColor": "#ffffffff",
      "androidScaleType": "CENTER_CROP",
      "androidSpinnerStyle": "large",
      "iosSpinnerStyle": "small",
      "spinnerColor": "#999999",
      "showSpinner": true
    }
  }
}
